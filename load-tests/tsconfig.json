{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["k6"],
    "paths": {
      "k6": ["./node_modules/@types/k6"],
      "k6/*": ["./node_modules/@types/k6/*"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
